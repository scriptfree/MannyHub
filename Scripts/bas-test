game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Success";
    Text = "Thanks for using Manny Hub!";
    Duration = 2;
})

local Library = loadstring(game:HttpGet("https://pastebin.com/raw/GdN5iRbt"))()
local Window = Library.CreateLib("Manny Hub", "BloodTheme")

local Tab1 = Window:NewTab("Main")
local Tab1Section = Tab1:NewSection("Configs")

local x = -252
local y = -8 -- starting floor is -8
local z = 48 -- Change this (by increments of 4) to change where the location of the wall is
local amount = 15
local waitTime = 0.01

-- Define necessary variables
local function define()
    local plr = game:GetService('Players').LocalPlayer
    local root = plr.Character.HumanoidRootPart
    return plr, root
end

-- Wait until the game is ready and define variables
print('loading')
local success, plr, root
repeat
    success, plr, root = pcall(define)
    wait()
until success

-- Function to kill monsters past the loot drop point
local function killMonsters()
    for _, monster in ipairs(workspace.Monsters:GetChildren()) do
        if monster:IsA("Model") and monster:FindFirstChild("HumanoidRootPart") then
            local monsterPos = monster.HumanoidRootPart.Position
            if monsterPos.X > x and monsterPos.Y > y and monsterPos.Z > z then
                monster:Destroy()
            end
        end
    end
end

Tab1Section:NewToggle("KILL ALL MONSTERS", ".", function(t)
    getgenv().FLO = t
    while getgenv().FLO do
        killMonsters()
        wait(waitTime)
    end
end)
